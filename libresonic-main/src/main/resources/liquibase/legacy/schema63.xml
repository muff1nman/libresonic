<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="schema63_001" author="muff1nman">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">select count(*) from version where version = 28</sqlCheck>
        </preConditions>
        <insert tableName="version">
            <column name="version" valueNumeric="28" />
        </insert>
        <rollback>
            <delete tableName="version" >
                <where>version = 28</where>
            </delete>
        </rollback>
    </changeSet>
    <changeSet id="schema63_002" author="muff1nman">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="media_file" columnName="checksum" />
            </not>
        </preConditions>
        <addColumn tableName="media_file">
            <column name="checksum" type="varchar(256)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
